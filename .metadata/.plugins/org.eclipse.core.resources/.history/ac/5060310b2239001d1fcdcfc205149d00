package variables;

public class Main {

	public static void main(String[] args) {
	//case sensitive
		int ogrenciSayisi = 12;
		System.out.println("Öğrenci Sayım : " + ogrenciSayisi);
		System.out.println("Öğrenci Sayım : " + ogrenciSayisi);
		System.out.println("Öğrenci Sayım : " + ogrenciSayisi);
		System.out.println("Öğrenci Sayım : " + ogrenciSayisi);
		System.out.println("Öğrenci Sayım : " + ogrenciSayisi);

		

	}

}
